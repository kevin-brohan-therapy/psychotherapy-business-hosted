# Cache Control Headers for GitHub Pages
# Cache static assets with fingerprinting for 1 year
/css/*.css
  Cache-Control: public, max-age=31536000, immutable

/js/*.js
  Cache-Control: public, max-age=31536000, immutable

/fonts/*
  Cache-Control: public, max-age=31536000, immutable

# Cache images for 1 month
/img/*
  Cache-Control: public, max-age=2592000

# Cache HTML pages for 1 hour
/*.html
  Cache-Control: public, max-age=3600

# Cache home and variant index pages for 30 minutes (for A/B testing updates)
/index.html
  Cache-Control: public, max-age=1800

/a/index.html
  Cache-Control: public, max-age=1800

/b/index.html
  Cache-Control: public, max-age=1800

/online/index.html
  Cache-Control: public, max-age=1800

/online/a/index.html
  Cache-Control: public, max-age=1800

/online/b/index.html
  Cache-Control: public, max-age=1800

/local/index.html
  Cache-Control: public, max-age=1800

/local/a/index.html
  Cache-Control: public, max-age=1800

/local/b/index.html
  Cache-Control: public, max-age=1800
